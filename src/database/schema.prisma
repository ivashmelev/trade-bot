generator client {
  provider = "prisma-client-js"
}

datasource db {
  url      = "postgresql://user:pass@localhost:5432/db"
  provider = "postgresql"
}

model User {
  id         Int    @id @default(autoincrement())
  telegramId Int
  name       String
}

model Order {
  id            Int      @id @default(autoincrement())
  time          DateTime
  type          String
  price         String
  stopPrice     String
  quantity      String
  quoteQuantity String
  side          String
  symbol        String
}

model Error {
  id      Int     @id @default(autoincrement())
  time    Int
  message String?
  url     String?
}

model StopLossOrder {
  id       Int    @id @default(autoincrement())
  price    String
  quantity String
}
